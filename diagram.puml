@startuml temp

' Define the entity for Products
table "Products" as Products {
    id INT PK "Primary Key"
    title VARCHAR(255) "Title of the product"
    price DECIMAL(10,2) "Price of the product"
}

' Define the entity for Images
table "Images" as Images {
    id INT PK "Primary Key"
    file_name VARCHAR(255) "Name of the image file"
    data BLOB "Binary data of the image"
    product_id INT FK "Foreign Key referencing Products"
}

' Define the relationship between Products and Images
Products ||--o{ Images : "has"

@enduml
